// The Heap
digraph {
	"->0" [label="{'A': 1}"]
	"->1" [label="{'B': 2}"]
	"->2" [label="{'C': 3}"]
	"->3" [label="{'D': 4}"]
	"->4" [label="{'E': 5}"]
	"->5" [label="{'F': 6}"]
	"->6" [label="{'G': 7}"]
	"->0" -> "->1"
	"->0" -> "->2"
	"->1" -> "->3"
	"->1" -> "->4"
	"->2" -> "->5"
	"->2" -> "->6"
}
