	"->0" [label=9]
	"->1" [label=8]
	"->2" [label=7]
	"->3" [label=4]
	"->4" [label=5]
	"->5" [label=6]
	"->6" [label=3]
	"->7" [label=2]
	"->8" [label=1]
	"->0" -> "->1"
	"->0" -> "->2"
	"->1" -> "->3"
	"->1" -> "->4"
	"->2" -> "->5"
	"->2" -> "->6"
	"->3" -> "->7"
	"->3" -> "->8"
}
